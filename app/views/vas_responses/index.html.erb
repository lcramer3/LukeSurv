<p id="notice"><%= notice %></p>

<h1>Listing Vas Responses</h1>

<table>
  <thead>
    <tr>
      <th>Value</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vas_responses.each do |vas_response| %>
      <tr>
        <td><%= vas_response.value %></td>
        <td><%= vas_response.user %></td>
        <td><%= link_to 'Show', vas_response %></td>
        <td><%= link_to 'Edit', edit_vas_response_path(vas_response) %></td>
        <td><%= link_to 'Destroy', vas_response, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vas response', new_vas_response_path %>
